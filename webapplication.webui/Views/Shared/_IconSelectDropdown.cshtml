    "fa-twitter",
    "fa-image",
    "fa-heart",
    "fa-cloud",
    "fa-youtube",
    "fa-circle-xmark",
    "fa-house",
    "fa-phone",
    "fa-bars",
    "fa-envelope",
    "fa-star",
    "fa-comment",
    "fa-arrow-up",
    "fa-arrow-down",
    "fa-arrow-left",
    "fa-arrow-right",
    "fa-file",
    "fa-bell",
    "fa-umbrella",
    "fa-gift",
    "fa-pen",
    "fa-trash",
    "fa-barcode",
    "fa-windows",
